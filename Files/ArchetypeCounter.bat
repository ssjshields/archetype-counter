        {        
            IntPtr hdcBitmap = gfxBmp.GetHdc();       

            try
            {
                PrintWindow(hwnd, hdcBitmap, PW_CLIENTONLY | PW_RENDERFULLCONTENT);
            }
            finally
            {
                gfxBmp.ReleaseHdc(hdcBitmap);
                gfxBmp.Dispose();
            }
        }


