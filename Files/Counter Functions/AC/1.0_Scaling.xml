<?xml version="1.0" encoding="UTF-8"?>
<themes>
    <fontDef name="Counter_Monster_Name" filename="NotoSans-Medium.ttc" color="#ffffff" border_width="1.1" border_color="#000000" size="18" hinting="Full" offsetY="-3"/><fontGen/>
	<theme name="redlabel" ref="redlabel" allowWildcard="true">
		<param name="border"><border>0</border></param>
		<param name="font"><font>Counter_Monster_Name</font></param>
	</theme>

	<images file="Base_Dot.png" filter="nearest">
		<area name="Screen_Filter" xywh="*" tint="#04ffffff"/>
		<area name="Battle_Pattern" xywh="*" splitx="L6,R0" splity="T6,B0" inset="-1,-1,1,1"/>
	</images>
	<theme name="battlegui" ref="battlegui">
		<param name="background"><image>Screen_Filter</image></param>
		<param name="overlay"><image>Battle_Pattern</image></param>
	</theme>

	<images>
		<composed name="battle-ui-enemy"><alias ref="battle-ui-enemy" tint="#fbffff"/></composed>
		<composed name="battle-ui-float"><alias ref="battle-ui-float" tint="#fbffff"/></composed>
		<composed name="battle-ui-self"><alias ref="battle-ui-self" tint="#fbffff"/></composed>
		<composed name="battle-ui-self-slim"><alias ref="battle-ui-self-slim" tint="#fbffff"/></composed>
	</images>
	<theme name="battle-gui-enemy" ref="battle-gui-enemy"><param name="background"><image>battle-ui-enemy</image></param></theme>
	<theme name="battle-gui-float" ref="battle-gui-float"><param name="background"><image>battle-ui-float</image></param></theme>
	<theme name="battle-gui-self" ref="battle-gui-float"><param name="background"><image>battle-ui-self</image></param></theme>
	<theme name="battle-gui-self-slim" ref="battle-gui-float"><param name="background"><image>battle-ui-self-slim</image></param></theme>

	<images file="Speech_Dot.png" filter="nearest">
		<area name="Speech_Pattern" xywh="*" splitx="L6,R0" splity="T6,B0" inset="-1,-1,1,1"/>
		<area name="Spectator_Filter" xywh="*" tint="#02ffffff" inset="-10000"/>
	</images>
	<theme name="messagebox-bubble" ref="messagebox-bubble">
		<param name="overlay"><image>Speech_Pattern</image></param>
	</theme>
	<theme name="time-progressbar" ref="-defaults" allowWildcard="true">
		<param name="overlay"><image>Spectator_Filter</image></param>
	</theme>
</themes>